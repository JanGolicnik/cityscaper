{
  "rules": {
    "iterations": 4,
    "initial": "X",
    "rules": {
      "F": [
        {
          "result": "F",
          "chance": 0.1
        },
        {
          "result": "FF",
          "chance": 0.85
        },
        {
          "result": "FFF",
          "chance": 0.05
        }
      ],
      "X": [
        { "result": "F[^X][&X]FX", "chance": 0.5 },
        { "result": "F[^X]FX", "chance": 0.05 },
        { "result": "F[&X]FX", "chance": 0.05 },
        { "result": "F[^^X][&X]FX", "chance": 0.1 },
        { "result": "F[^X][&&X]FX", "chance": 0.1 },
        { "result": "F[^X][&X]FA", "chance": 0.2 }
      ]
    }
  },
  "rendering": {
    "default_angle_change": 15.0,
    "shapes": {
      "F": {
        "Line": {
          "width": 20,
          "length": 0.025,
          "angle": 20.0,
          "color": [0.5, 0.7, 0.1]
        }
      },
      "A": {
        "Circle": {
          "size": 0.01,
          "color": [1.0, 0.5, 0.2]
        }
      }
    }
  }
}
